<div class="container">
  <div class="wrapper col-md-8 col-md-offset-2 col-sm-10">
    <p><%= notice %></p>
    <h2>代理承認者一覧</h2>
      <%= link_to "新規登録", new_dmanager_path, class: 'btn btn-default' %>
      <%= link_to "csv出力", dmanagers_path(format: :csv), class: "btn btn-primary" %>
      <div class="row">
        <table class = "table table-hover">
           <tr>
             <th>対象社員番号</th>
             <th>対象社員氏名</th>
             <th>代理承認者番号</th>
             <th>代理承認者氏名</th>
             <th>権限付与日</th>
             <th></th>
             <th>情報変更日時</th>
           </tr>
          <% @dmanagers.each do |t| %>
           <tr>
             <td><%= t.emp.employeecode %></td>
             <td><%= t.emp.name %></td>
             <td><%= t.maneger.employeecode %></td>
             <td><%= t.maneger.name %></td>
             <td><%= t.start_date.strftime("%Y/%m/%d") %></td>
             <td><%= link_to "編集", edit_dmanager_path(t.id), class: 'btn btn-default btn-sm btn-success' %></td>
             <td><%= t.updated_at.strftime("%Y/%m/%d %H:%M:%S") %></td>
           </tr>
          <% end %>
        </table>
      </div>
  </div>
</div>
